{
  "name": "briq-builder",
  "version": "1.7.0-alpha",
  "description": "Frontend builder for the briq protocol",
  "homepage": "https://github.com/briqNFT/briq-builder/",
  "scripts": {
    "dev": "vite",
    "build": "vite build --outDir dist/ && vite build --ssr server/main.ts --outDir dist/server",
    "serve": "vite preview",
    "test": "vitest run",
    "coverage": "vitest run --coverage",
    "dev-server": "vite build --ssr server/main.ts --outDir dist/server -w -m development",
    "build-server": "vite build --ssr server/main.ts --outDir dist/server",
    "server": "node dist/server/main.js"
  },
  "dependencies": {
    "@jaames/iro": "^5.5",
    "@sentry/tracing": "^6.16",
    "@sentry/vue": "^6.16",
    "chart.js": "^3.7.1",
    "connect": "^3.7",
    "fflate": "^0.7.3",
    "get-starknet-wallet": "github:briqNFT/get-starknet-wallet#soft-connect",
    "json-bigint": "git+https://github.com/briqNFT/json-bigint.git#fix-ts",
    "starknet": "^3.9",
    "three": "^0.138",
    "uuid": "^8",
    "vite": "^2.6",
    "vox-reader": "git+https://github.com/FlorianFe/vox-reader.js.git",
    "vue": "^3.2",
    "vue-router": "^4.0",
    "vuex": "^4.0"
  },
  "devDependencies": {
    "@types/connect": "^3.4",
    "@types/node": "^17.0",
    "@types/three": "^0.138",
    "@types/uuid": "^8.0",
    "@vitejs/plugin-vue": "^2",
    "autoprefixer": "^10.4",
    "c8": "^7.11.0",
    "postcss": "^8.4",
    "rollup-plugin-analyzer": "^4.0.0",
    "rollup-plugin-visualizer": "^5.6.0",
    "tailwindcss": "^3.0",
    "vite-svg-loader": "^3.1",
    "vitest": "^0.9.3"
  }
}
